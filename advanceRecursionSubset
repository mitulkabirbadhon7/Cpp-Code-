#include <bits/stdc++.h>
using namespace std;
void solve(string ip , string op,set<string>&ans){

	if(ip.length() == 0){
		
		ans.insert(op);
		return;
	}
	
	string op1 = op;
	string op2 = op;
	
	op2.push_back(ip[0]);
	ip.erase(ip.begin()+0);
	solve(ip,op1,ans);
	solve(ip,op2,ans);
	return;	
	
}
int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    string ip;
    cin >> ip;
    
    string op ="";
    set<string>ans;
    solve(ip,op,ans);
    for(auto s : ans){
    	cout<<s<<" ";
    }
    

    return 0;
}
